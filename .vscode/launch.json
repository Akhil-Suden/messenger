{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Go Program",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/main.go",
      "env": {},
      "args": [],
      "cwd": "${workspaceFolder}/cmd",
      "buildFlags": ""
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}/web"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Chrome against lan ip",
      "url": "http://192.168.1.102:8080",
      "webRoot": "${workspaceFolder}/web"
    }
  ]
}